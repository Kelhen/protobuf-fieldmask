{
  "name": "protobuf-fieldmask",
  "version": "1.0.0",
  "description": "Library for generating and applying FieldMask",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "scripts": {
    "test": "mocha ./test",
    "test-coverage": "nyc npm test",
    "lint": "eslint .",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\"",
    "test-travis": "npm run lint && nyc mocha --opts \"./mocha.opts\" test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/protobuf-fieldmask.git"
  },
  "keywords": [
    "protocol",
    "buffers",
    "protobuf",
    "field",
    "mask",
    "json",
    "grpc",
    "fieldmask"
  ],
  "homepage": "http://github.com/kibertoad/protobuf-fieldmask",
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.7"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "lib/**"
    ],
    "cache": true,
    "all": true,
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  }
}
